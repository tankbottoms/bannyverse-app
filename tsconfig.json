{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$data": ["src/data"],
			"$data/*": ["src/data/*"],
			"$stores": ["src/stores"],
			"$stores/*": ["src/stores/*"],
			"$utils": ["src/utils"],
			"$utils/*": ["src/utils/*"],
			"$assets": ["src/assets"],
			"$assets/*": ["src/assets/*"],
			"$constants": ["src/constants"],
			"$constants/*": ["src/constants/*"]
		}
	}
}
